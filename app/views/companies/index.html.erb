<p id="notice"><%= notice %></p>

<h1>Companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Website</th>
      <th>Owns</th>
      <th>Rents</th>
      <th>Industry</th>
      <th>Sf using</th>
      <th>Sic code</th>
      <th>Source of name</th>
      <th>Suite</th>
      <th>Total employees</th>
      <th>Location requirements</th>
      <th>Year founded</th>
      <th>Data checked</th>
      <th>Exclussive</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= company.phone %></td>
        <td><%= company.fax %></td>
        <td><%= company.website %></td>
        <td><%= company.owns %></td>
        <td><%= company.rents %></td>
        <td><%= company.industry %></td>
        <td><%= company.SF_using %></td>
        <td><%= company.sic_code %></td>
        <td><%= company.source_of_name %></td>
        <td><%= company.suite %></td>
        <td><%= company.total_employees %></td>
        <td><%= company.location_requirements %></td>
        <td><%= company.year_founded %></td>
        <td><%= company.data_checked %></td>
        <td><%= company.exclussive %></td>
        <td><%= company.active %></td>
        <td><%= link_to 'Show', company %></td>
        <td><%= link_to 'Edit', edit_company_path(company) %></td>
        <td><%= link_to 'Destroy', company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company', new_company_path %>
